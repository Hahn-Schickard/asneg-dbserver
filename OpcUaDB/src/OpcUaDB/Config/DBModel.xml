<?xml version="1.0" encoding="utf-8"?>
<DBModel Id="Client" xmlns="http://ASNeG.de/DBModel.xsd">

  <Database>
  
    <Name>PostgreSQL TestData</Name>

  </Database>

  <OpcUaAccess>
		
    <NamespaceUris>
      <Uri>http://ASNeG.de/DBServer/</Uri>
	</NamespaceUris>
	
	<IdentAccess>
      <Server Service="Call" NodeId="ns=1;s=IdentAccessString"/>
      
      <SQLQuerys>
        <SQLQuery Id="TestData1">select * from TestData</SQLQuery>
        <SQLQuery Id="TestData2">select * from TestData where Field1 > %1</SQLQuery>
        <SQLQuery Id="TestData3">select * from TestData where Field1 > %1 AND Field2 > %2</SQLQuery>
      </SQLQuerys>
      
	</IdentAccess>
	
	<SQLAccess>
      <Server Service="Call" NodeId="ns=1;s=SQLAccessString"/>
	</SQLAccess>
	
  </OpcUaAccess>

</DBModel>